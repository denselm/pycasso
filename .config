[File]
# File operations
save_image=1
external_image_location=images/external
generated_image_location=images/generated
image_format=png
font_file=fonts/Font.ttc
subjects_file=prompts/subjects.txt
artists_file=prompts/artists.txt
prompts_file=prompts/prompts.txt

[EPD]
# EPD information - used by omni-epd
type=omni_epd.mock
mode=color

[Display]
# Display modifications - used by omni-epd
rotate = 0
flip_horizontal=False
flip_vertical=False

[Image Enhancements]
# Image modifications - used by omni-epd
contrast=1
brightness=1
sharpness=1

[Prompt]
# Automatic prompt construction
# Mode 1 - (preamble - subjects.txt - connector - artists.txt - postscript)
# Mode 2 - (preamble - prompts.txt - postscript)
# Mode 0 - Any of the above
mode=1
preamble=
connector=by
postscript=, digital art, trending on artstation

[Text]
# Text parsing
add_text = True
parse_text = True
preamble_regex =  .* -
artist_regex =  in the style of
remove_text = , digital art
            A painting of
            an oil painting of
            a surrealist oil painting of
            graffiti of
box_to_floor = True
box_to_edge = True
artist_loc = 10
artist_size = 14
title_loc = 28
title_size = 20
padding = 10
opacity = 150

[Icon]
icon_padding = 5
icon_size = 10
icon_width = 3
icon_opacity = 150

[Logging]
# Logging settings
log_file = pycasso.log
# Levels: CRITICAL:50, ERROR:40, WARNING:30, INFO:20, DEBUG:10, NOTSET:0
log_level = 10

[Providers]
# Provider weight - comparative chance whether any particular provider is used
external_amount = 0
historic_amount = 1
stability_amount = 0
dalle_amount = 0