[File]
# File operations
image_location = images
image_format = png
font_file = fonts/Font.ttc

[EPD]
# EPD information - used by omni-epd
type=omni_epd.mock
mode=color

[Display]
# Display modifications - used by omni-epd
rotate = 0
flip_horizontal=False
flip_vertical=False

[Image Enhancements]
# Image modifications - used by omni-epd
contrast=1
brightness=1
sharpness=1

[Text]
# Text parsing
add_text = True
parse_text = True
preamble_regex =  .* -
artist_regex =  in the style of
remove_text = , digital art
            A painting of
            an oil painting of
            a surrealist oil painting of
            graffiti of
box_to_floor = True
box_to_edge = True
artist_loc = 10
artist_size = 14
title_loc = 28
title_size = 20
padding = 10
opacity = 150

[Providers]
# Provider weight - comparative chance whether any particular provider is used
historic_amount = 1
stability_ai_amount = 1
dalle_amount = 0

[DEBUG]
# Settings for debug
image_viewer = True

